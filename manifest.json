{
  "description": "Shows the last downloaded item, and lets you open or delete it",
  "manifest_version": 2,
  "name": "latest-download-kotlin",
  "version": "1.0",
  "homepage_url": "https://github.com/",
  "icons": {
    "48": "out/production/resources/icons/page-48.png",
    "32": "out/production/resources/icons/page-32.png"
  },

  "permissions": [
    "downloads",
    "downloads.open"
  ],

  "browser_action": {
    "browser_style": true,
    "default_title": "Latest download",
    "default_popup": "out/production/resources/html/index.html"
  }

}